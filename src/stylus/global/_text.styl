html
  font-family $font-family
  
.text-white
  color white
  
.text-center
  text-align center
  
.text-underline
  text-decoration underline
  
.text-uppercase
  text-transform uppercase

.text-left
  text-align left
.text-right
  text-align right

.text-bold
  font-weight bold

.text-ellipsis
  text-overflow ellipsis
  white-space nowrap
  overflow hidden

.text-size-zero
  font-size 0

.text-blue
  color #354967


  
